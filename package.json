{
  "scripts": {
    "bootstrap": "pnpm i",
    "dev": "cross-env VITE_MICRO_MODE=true redbud run ./scripts/dev.ts",
    "dev:main": "pnpm run dev --app main",
    "dev:subapp1": "pnpm run dev --app subapp1",
    "dev:subapp2": "pnpm run dev --app subapp2",
    "dev:subapp3": "pnpm run dev --app subapp3",
    "build": "cross-env VITE_MICRO_MODE=true redbud run ./scripts/build.ts",
    "build:plugin": "pnpm run build --app plugin",
    "build:main": "pnpm run build --app main",
    "build:subapp1": "pnpm run build --app subapp1",
    "build:subapp2": "pnpm run build --app subapp2",
    "build:subapp3": "pnpm run build --app subapp3",
    "lint": "eslint --ext .js,.ts,.vue --fix ./packages/*/src",
    "prettier": "prettier --write --ignore-unknown '**/*'",
    "commit": "git-cz",
    "release": "npx bumpp --push --tag --commit 'release: v'",
    "prepare": "husky install",
    "docker:build": "docker build --progress=plain -t micro-frontend-starter .",
    "docker:up": "docker-compose up -d"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "bumpp": "^8.2.1",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "prettier": "^2.7.1",
    "redbud": "^1.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0"
  }
}
